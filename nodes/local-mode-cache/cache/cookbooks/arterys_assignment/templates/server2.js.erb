// modules
var static = require( 'node-static' ), port = 8080,http = require( 'http' );

//Read the logfile
var logFile = require('fs');
logFile.readFile('/tmp/output.log', function (err, data) {
    if (err) throw err;
});

//Display the logfile
var fileServer = new static.Server(logFile);

http.createServer(function (req, res) {

    fileServer.serve(req, res);

}).listen(port);
